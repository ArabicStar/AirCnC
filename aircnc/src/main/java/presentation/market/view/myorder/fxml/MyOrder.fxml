<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="768.0" prefWidth="777.0" stylesheets="@../css/MyOrder.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.market.view.myorder.fxml.MyOrderController">
   <children>
      <HBox alignment="CENTER" layoutX="53.0" layoutY="26.0" spacing="40.0">
         <children>
            <CheckBox fx:id="unfinished" layoutX="214.0" layoutY="107.0" mnemonicParsing="false" text="未执行订单" />
            <CheckBox fx:id="finished" layoutX="330.0" layoutY="98.0" mnemonicParsing="false" text="已执行订单" />
            <CheckBox fx:id="exception" layoutX="340.0" layoutY="108.0" mnemonicParsing="false" text="异常订单" />
            <CheckBox fx:id="cancelled" layoutX="350.0" layoutY="118.0" mnemonicParsing="false" text="撤销订单" />
            <Button fx:id="query" mnemonicParsing="false" onAction="#handleQuery" styleClass="check" text="查询" />
         </children>
      </HBox>
      <TableView fx:id="orderTable" layoutX="44.0" layoutY="143.0" prefHeight="471.0" prefWidth="702.0">
        <columns>
          <TableColumn fx:id="userName" prefWidth="75.0" styleClass="table-row-cell" text="用户名称" />
            <TableColumn fx:id="userId" prefWidth="75.0" styleClass="table-row-cell" text="用户ID" />
            <TableColumn fx:id="orderId" prefWidth="75.0" styleClass="table-row-cell" text="订单号" />
          <TableColumn fx:id="checkInTime" prefWidth="75.0" styleClass="table-row-cell" text="入住时间" />
            <TableColumn fx:id="status" prefWidth="75.0" styleClass="table-row-cell" text="状态" />
            <TableColumn fx:id="totalPrice" prefWidth="75.0" styleClass="table-row-cell" text="总价" />
            <TableColumn fx:id="operation" prefWidth="75.0" styleClass="table-row-cell" text="操作" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Label layoutX="27.0" layoutY="63.0" prefHeight="62.0" prefWidth="169.0" styleClass="title" text="搜索结果" />
   </children>
</Pane>
