<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="768.0" prefWidth="1024.0" style="-fx-background-color: #000;" stylesheets="@MarketMain.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.market.view.MarketMainController">
   <left>
      <Pane prefHeight="768" prefWidth="247.0" BorderPane.alignment="CENTER">
         <children>
            <VBox alignment="CENTER" layoutY="274.0" prefHeight="268.0" prefWidth="247.0">
               <children>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="121.0" styleClass="hbox" onMouseClicked="#handleWebsitePromotionStrategy">
                     <children>
                        <Label fx:id="websitePromotionStrategy" contentDisplay="CENTER"  text="网站营销策略" textAlignment="CENTER" />
                     </children>
                     <padding>
                        <Insets right="30.0" />
                     </padding>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="121.0" styleClass="hbox" onMouseClicked="#handleAbnormalOrderBrowse">
                     <children>
                        <Label fx:id="abnormalOrderBrowse"  text="异常订单浏览" textAlignment="CENTER" />
                     </children>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <padding>
                        <Insets right="30.0" />
                     </padding>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="121.0" styleClass="hbox" onMouseClicked="#handleCreditTopup">
                     <children>
                        <Label fx:id="creditTopup"  text="信用充值" textAlignment="CENTER" />
                     </children>
                     <padding>
                        <Insets right="30.0" />
                     </padding>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="121.0" styleClass="hbox" onMouseClicked="#handleMyOrder">
                     <children>
                        <Label fx:id="myOrder"  text="我的订单" textAlignment="CENTER" />
                     </children>
                     <padding>
                        <Insets right="30.0" />
                     </padding>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="121.0" styleClass="hbox" onMouseClicked="#handleSearchHotel">
                     <children>
                        <Label fx:id="searchHotel"  text="搜索酒店" textAlignment="CENTER" />
                     </children>
                     <padding>
                        <Insets right="30.0" />
                     </padding>
                  </HBox>
               </children>
            </VBox>
         </children>
      </Pane>
   </left>
</BorderPane>
